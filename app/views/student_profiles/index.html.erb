
<p id="notice"><%= notice %></p>



  <table id="example" class="table table-striped table-bordered" width="100%" cellspacing="0">
   
  <thead>
    <tr>
      <th>Name</th>
      <th>Contact</th>
      <th>College</th>
      <th>Address</th>
      <th>Branch</th>
      <th>Current year</th>
      <th>Profile pic</th>
      <th>Gender</th>
      <th>Actions</th>
      <th>Actions</th>
      <th>Actions</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  

  <tbody>
    <% @student_profiles.each do |student_profile| %>
      <tr>
        <td><%= student_profile.name %></td>
        <td><%= student_profile.contact %></td>
        <td><%= student_profile.college %></td>
        <td><%= student_profile.address %></td>
        <td><%= student_profile.branch %></td>
        <td><%= student_profile.current_year %></td>
        <td><%= student_profile.profile_pic %></td>
        <td><%= student_profile.gender %></td>
        <td><%= link_to 'Show', student_profile %></td>
        <td><%= link_to 'Edit', edit_student_profile_path(student_profile) %></td>
        <td><%= link_to 'Destroy', student_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Profile', new_student_profile_path %>

<script>
$(document).ready(function() {
    $('#example').DataTable();
} );
</script>